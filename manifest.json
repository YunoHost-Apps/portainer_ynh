{
    "name": "Portainer",
    "id": "portainer",
    "url": "https://portainer.io/",
    "packaging_format": 1,
    "description": {
        "en": "Web interface for the Docker engine management"
    },
    "version": "2.11.0~ynh1",
    "url": "https://portainer.io",
    "upstream": {
        "license": "AGPL-3.0",
        "website": "https://portainer.io",
        "demo": "https://www.portainer.io/live-demo/",
        "admindoc": "https://docs.portainer.io/",
        "code": "https://github.com/portainer/portainer"
    },
    "license": "AGPL-3.0",
    "maintainer": {
        "name": "aymhce",
        "email": "aymhce@gmail.com"
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": false,
    "services": [
        "nginx"
     ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
